<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="263dp"
        android:layout_marginTop="468dp"
        android:background="@drawable/background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/NombreEntry"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="200dp"
        android:ems="10"
        android:fontFamily="@font/avenirbook"
        android:hint="Nombre completo:"
        android:inputType="textPersonName"
        android:textColor="@color/letra_aso_azul"
        android:textColorHint="@color/letra_aso_azul"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ContraEntry"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="256dp"
        android:layout_marginEnd="81dp"
        android:ems="10"
        android:fontFamily="@font/avenirbook"
        android:hint="Contraseña:"
        android:inputType="textPersonName"
        android:textColor="@color/letra_aso_azul"
        android:textColorHint="@color/letra_aso_azul"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/IngresarBoton"
        android:layout_width="224dp"
        android:layout_height="47dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="456dp"
        android:layout_marginEnd="94dp"
        android:background="@drawable/ingresar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="304dp"
        android:layout_height="109dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="54dp"
        android:fontFamily="@font/avenirblack"
        android:text="@string/app_name"
        android:textAlignment="center"
        android:textColor="@color/letra_aso_azul"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/info2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="675dp"
        android:background="@drawable/info"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="236dp"
        android:layout_height="68dp"
        android:layout_marginStart="89dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="91dp"
        android:fontFamily="@font/avenirbook"
        android:text="Ingresa sesión con la cuenta proporcionada por el dirigente"
        android:textAlignment="center"
        android:textColor="@color/letra_aso_azul"
        android:textSize="16sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Olvido"
        android:layout_width="190dp"
        android:layout_height="29dp"
        android:layout_marginStart="111dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="111dp"
        android:fontFamily="@font/avenirbook"
        android:text="¿Se te olvido la contraseña?"
        android:textColor="@color/letra_aso_red"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="340dp"
        android:layout_marginEnd="92dp"
        android:buttonTint="@color/letra_aso_red"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="84dp"
        android:layout_height="37dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="348dp"
        android:background="@drawable/logowak"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="73dp"
        android:layout_height="56dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="340dp"
        android:background="@drawable/logotropa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="340dp"
        android:buttonTint="@color/tropa_normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="136dp"
        android:layout_marginTop="340dp"
        android:buttonTint="@color/wak_normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="93dp"
        android:layout_height="44dp"
        android:layout_marginTop="348dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/logocomu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>